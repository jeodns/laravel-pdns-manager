{
    "name": "jeodns/pdns-manager",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jeodns\\PDNSManager\\": "src/",
            "Jeodns\\Models\\": "src/Models/",
            "Jeodns\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jeodns\\PDNSManager\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-yaml": "^2.2.2",
        "jeodns/wonder-network": "@dev",
        "jeodns/location-selector": "@dev",
        "dnj/local-filesystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.4.1",
        "friendsofphp/php-cs-fixer": "^3.13",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.19"
    },
    "scripts": {
        "test:types": "vendor/bin/phpstan analyze",
        "test:phpunit": "vendor/bin/phpunit",
        "test:codestyle": "vendor/bin/php-cs-fixer fix -v",
        "test": [
            "@test:types",
            "@test:phpunit",
            "@test:codestyle"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jeodns\\PDNSManager\\ServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../wonder-network"
        },
        {
            "type": "path",
            "url": "../location-selector"
        }
    ]
}
